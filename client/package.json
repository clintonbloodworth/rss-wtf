  },
  "engines": {
    "node": "^12"
  },
  "name": "@wtf/client",
  "private": true,
  "scripts": {
    "build": "per-env",
    "build:development": "webpack serve",
    "build:production": "webpack",
    "format": "prettier --write '**/*.(css|html|json|md)'",
    "lint": "per-env",
    "lint:development": "chokidar **/*.svelte --command 'stylelint {path} --allow-empty-input --fix' & npm run lint:scripts -- --changed --fix --quiet --watch",
    "lint:production": "npm run lint:styles && npm run lint:scripts",
    "lint:scripts": "esw . 'source/**/*.svelte' 'source/**/**/*.svelte' 'source/**/**/**/*.svelte'",
    "lint:styles": "stylelint **/*.svelte",
    "precommit": "lint-staged",
    "start": "per-env",
    "start:development": "concurrently --kill-others npm:lint npm:test npm:build",
    "start:production": "npm run lint && npm run test && npm run build",
    "test": "per-env",
    "test:development": "ava --watch",
    "test:production": "ava"
  }
}
