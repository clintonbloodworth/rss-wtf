  "name": "@wtf/server",
  "private": true,
  "scripts": {
    "format": "prettier --write '**/*.(html|json|md)'",
    "format:comment": "https://github.com/prettier/prettier/issues/8233#issuecomment-698884492",
    "lint": "dotenv-extended per-env",
    "lint:development": "esw *.js library/**/*.js --color --fix --watch",
    "lint:production": "esw *.js library/**/*.js --color --fix",
